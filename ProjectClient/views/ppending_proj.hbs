<table id="orders">
      <tr>
        <th>Project Type</th>
        <th>Project Name</th>
        <th>Project ID</th>
        <th>Contractor ID</th>
        <th>Contractor Public Key</th>
        <th>Task</th>
        <th>Amount</th>
        <th>Bill Uploaded</th>
        <th>Bill</th>
        <th>Onsite Engineer Approved</th>
        <th>Project View</th>
        <th>Project Report</th>
        <th></th>
      </tr>
      {{#listings}}
      <tr class="">
        <td>{{data.project_type}}</td>
        <td>{{data.proj_name}}</td>
        <td>{{data.proj_id}}</td>
        <td>{{data.con_id}}</td>
        <td>{{data.con_key}}</td>
        <td>{{data.task}}</td>
        <td>{{data.bidamount}}</td>
        <td>{{data.cuploaded}}</td>
        <td><a href={{data.bill}}>Bill Link</a></td>
        <td>{{data.eapproved}}</td>
        <td><a href={{data.picture}}>Picture </td>
        <td><a href={{data.report}}>Project Report </td>
        <td>
          <center>
            <button class="button" onclick="approve_proj(event,'{{address}}')">
              Approve
            </button>
          </center>
        </td>
      </tr>
      {{/listings}}
</table>
